{
  "name": "npx",
  "version": "0.0.0",
  "license": "MIT",
  "bin": "cli.js",
  "main": "./dist/cli.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "docker-compose": "echo \"Running in docker-compose... You should run interactively by attaching:\n\t\tdocker exec -it `hostname` npm run start\" && tail -f /dev/null",
    "start": "parcel build ./cli.js && npx ./dist/cli.js",
    "dev": "concurrently -r 'npm:dev-concurrently*'",
    "dev-concurrently:watch": "parcel watch ./cli.js",
    "dev-concurrently:run": "nodemon --inspect=0.0.0.0:9229 -L ."
  },
  "dependencies": {
    "@parcel/transformer-react-refresh-wrap": "^2.0.0-alpha.3",
    "arr-rotate": "^1.0.0",
    "chalk": "^4.1.2",
    "conf": "^10.0.1",
    "figlet": "^1.5.0",
    "import-jsx": "^4.0.0",
    "ink": "^3.0.9",
    "ink-form": "^1.0.1",
    "ink-progress-bar": "^3.0.0",
    "ink-select-input": "^4.2.0",
    "ink-spinner": "^4.0.2",
    "ink-text-input": "^4.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "meow": "^9.0.0",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-useinterval": "^1.0.2",
    "tinygradient": "^1.1.5",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.pick": "^4.4.6",
    "@types/react": "^17.0.15",
    "concurrently": "^6.2.0",
    "eslint-config-xo-react": "^0.25.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ink-testing-library": "^2.1.0",
    "nodemon": "^2.0.12",
    "parcel": "^2.0.0-beta.3.1"
  },
  "alias": {
    "_Shared": "./src/_shared"
  }
}
